// https://codeforces.com/group/w0J26SHHez/contest/421918/problem/B
#include <iostream>
#include <vector>
#include <string>
#include <cmath>
#include <algorithm>
#include <set>
using namespace std;


int main() {
	int t;
	cin >> t;
	while (t--) {
		int n;
		cin >> n;
		int maxAns = 0;
		char cur_smb;
		for (int i = 0; i < n; i++) {
			cin >> cur_smb;
			maxAns = max(cur_smb - 'a' + 1, maxAns);
		}
		cout << maxAns << '\n';
	}
}
